#include <iostream>
#include <algorithm>
#include <iterator>
int A[] = {1, 3, 7, 21, 8};

class cmp
{
public:
    bool operator()(const int a, const int b) {
        return a != b;
    }
}k;

int main()
{
    std::cout << *std::max_element(std::begin(A), std::end(A), k) << std::endl;
    return 0;
}
